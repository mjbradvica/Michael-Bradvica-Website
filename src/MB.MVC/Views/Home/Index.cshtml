@model HomeViewModel
@{
    ViewData["Title"] = "Home Page";
}

<div class="pure-g">
    <div class="pure-u-1-5"></div>
    <div class="pure-u-3-5">
        @foreach (var post in Model.PostPartials)
        {
            await Html.RenderPartialAsync(post);
        }
    </div>
    <div class="pure-u-1-5"></div>
</div>